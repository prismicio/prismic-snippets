	value="&lt;PrismicEmbed :field=&quot;$1$&quot; wrapper=&quot;$2$&quot; /&gt;"
	description="Yields a ready-to-use `&lt;PrismicEmbed /&gt;` component"
	toReformat="true"
	toShortenFQNames="true"
