{
	"name": "prismic-snippets",
	"displayName": "Prismic.io snippets",
	"publisher": "prismicio",
	"version": "0.0.10",
	"description": "Prismic snippets for VS Code and Vim",
	"categories": [
		"Snippets"
	],
	"keywords": [
		"typescript",
		"prismic",
		"vscode",
		"vim",
		"coc.nvim"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/prismicio/prismic-snippets.git",
		"directory": "vscode"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"files": [
		"icon.png",
		"snippets"
	],
	"scripts": {
		"release": "vsce package && vsce publish && npm publish"
	},
	"contributes": {
		"snippets": [
			{
				"language": "html",
				"path": "./snippets/html.code-snippets"
			},
			{
				"language": "javascript",
				"path": "./snippets/javascript.code-snippets"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets/javascriptreact.code-snippets"
			},
			{
				"language": "typescript",
				"path": "./snippets/typescript.code-snippets"
			},
			{
				"language": "typescriptreact",
				"path": "./snippets/typescriptreact.code-snippets"
			},
			{
				"language": "vue",
				"path": "./snippets/vue.code-snippets"
			},
			{
				"language": "vue-html",
				"path": "./snippets/vue-html.code-snippets"
			}
		]
	},
	"devDependencies": {
		"vsce": "^2.7.0"
	},
	"engines": {
		"vscode": "^1.50.0",
		"coc": "^0.0.80"
	},
	"icon": "icon.png"
}
