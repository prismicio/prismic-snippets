{
	"name": "@prismicio/snippets",
	"version": "0.0.6",
	"description": "Prismic snippets for VS Code, Vim, Sublime, and IntelliJ",
	"private": true,
	"keywords": [
		"typescript",
		"prismic",
		"vscode",
		"vim",
		"sublime",
		"intellij"
	],
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/prismic-snippets.git"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"type": "module",
	"scripts": {
		"dev": "cross-env DEBUG=* node --experimental-json-modules scripts/dev",
		"build": "cross-env DEBUG=* node --experimental-json-modules scripts/build",
		"format": "prettier --write .",
		"prepare": "npm run build",
		"release": "npm run test && standard-version && git push --follow-tags && npm run build && npm run release -w vscode",
		"release:dry": "standard-version --dry-run",
		"lint": "eslint --ext .js,.ts .",
		"unit": "echo \"no unit test\"",
		"size": "echo \"no size test\"",
		"test": "npm run lint && npm run unit && npm run build && npm run size"
	},
	"engines": {
		"node": ">=16.10.0"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"chokidar": "3.5.3",
		"cross-env": "7.0.3",
		"debug": "4.3.4",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-tsdoc": "^0.2.14",
		"front-matter": "4.0.2",
		"globby": "11.1.0",
		"prettier": "^2.6.1",
		"prettier-plugin-jsdoc": "^0.3.33",
		"standard-version": "^9.3.2"
	},
	"workspaces": [
		"vscode"
	]
}
